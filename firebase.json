{
  "database": {
    "rules": "y"
  },
  "hosting": {
    "public": "re-public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/register.html"
      },
      {
        "source": "/",
        "destination": "/404.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
    
  }
}
